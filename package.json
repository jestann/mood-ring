{
  "name": "MoodRing",
  "author": "she-hacks-group",
  "description": "a tracker for anxiety and ptsd",
  "repository": "https://github.com/jestann/mood-ring.git",
  "main": "index.js",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "9.4.0",
    "npm": "5.6.0"
  },
  "dependencies": {
    "chart.js": "^2.9.4",
    "firebase": "^4.9.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-chartjs-2": "^2.7.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.0",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^4.8.0",
    "sass": "^1.0.0-beta.4"
  },
  "scripts": {
    "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-css": "node-sass-chokidar src/styles -o src/css",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
